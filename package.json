{
  "name": "vuejs-rollup-boilerplate",
  "version": "1.0.0",
  "description": "Vue.js Rollup boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c -w --environment NODE_ENV:dev,PORT:8080",
    "build": "rollup -c -w --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod",
    "postinstall": "rollup-watch-fix"
  },
  "author": "Dmitrii Goriunov",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "rollup-watch-fix": "0.0.3",
    "autoprefixer": "^7.2.4",
    "cssnano": "^3.10.0",
    "postcss": "^6.0.16",
    "rollup": "^0.53.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "vue-template-compiler": "^2.5.13"
  }
}